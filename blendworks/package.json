    "i18next-browser-languagedetector": "^7.2.0",
    "chart.js": "^4.4.1"
  },
  "devDependencies": {
    "vercel": "^32.7.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "@types/node": "^20.10.6"
  },
  "engines": {
    "node": ">=18.0.0"
