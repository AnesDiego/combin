    "format": "prettier --write assets/js/**/*.js assets/css/**/*.css",
    "analyze": "node scripts/analyze-bundle.js",
    "translations:extract": "node scripts/extract-translations.js",
    "translations:validate": "node scripts/validate-translations.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "stripe": "^14.15.0",
