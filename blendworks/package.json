{
  "name": "blendworks",
  "version": "1.0.0",
  "description": "Professional combination generator with global payments, multi-language support, and intelligent lead generation",
  "main": "index.html",
  "scripts": {
    "dev": "vercel dev",
    "build": "vercel build",
    "deploy": "vercel --prod",
    "start": "vercel dev",
